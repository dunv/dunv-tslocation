{
    "name": "dunv-tslocation",
    "version": "0.0.6",
    "description": "Location helpers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "tsc --noEmit && eslint 'src/*.{js,ts,tsx}' --quiet --fix",
        "clean": "rimraf build",
        "build": "tsc --build",
        "start": "tsc -w",
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
        "prepare": "npm run clean && npm run lint && npm run build",
        "createRelease": "npm version patch && git push --follow-tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dunv/dunv-tslocation.git"
    },
    "author": "Daniel Unverricht <daniel@unverricht.net>",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^8.2.0",
        "@types/qs": "^6.9.3",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "chai": "^4.2.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.20.3",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "rimraf": "^3.0.2",
        "ts-loader": "^8.0.14",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "qs": "^6.9.6"
    }
}
